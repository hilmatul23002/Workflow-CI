name: cs-mlflow-modelling

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: 100
        type: int
        default: 505
      max_depth:
        type: int
        default: 35
      dataset:
        type: string
        default: "datasekolahsiap.csv"
    command: "python modelling.py {n_estimators} {max_depth} {dataset}"

